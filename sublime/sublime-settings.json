{
  "always_show_minimap_viewport": true,
  "bold_folder_labels": true,
  "caret_extra_bottom": 0,
  "caret_extra_top": 0,
  "caret_extra_width": 8,
  "caret_style": "phase",
  "color_scheme":
    "Packages/Material Theme/schemes/Material-Theme-Darker.tmTheme",
  "dart_sdk_path": "/usr/local/opt/dart/libexec",
  "detect_indentation": true,
  "draw_indent_guides": false,
  "enable_tab_scrolling": false,
  "file_exclude_patterns": [
    "*.pyc",
    "*.pyo",
    "*.exe",
    "*.dll",
    "*.obj",
    "*.o",
    "*.a",
    "*.lib",
    "*.so",
    "*.dylib",
    "*.ncb",
    "*.sdf",
    "*.suo",
    "*.pdb",
    "*.idb",
    ".DS_Store",
    "*.class",
    "*.psd",
    "*.db",
    "*.sassc",
    "*.scssc",
    "*.map",
    "*.po",
    "*.mo",
    "yarn.lock",
    "package.lock"
  ],
  "folder_exclude_patterns": [
    ".fusebox",
    "node_modules",
    "build",
    "dist",
    "output",
    "typings",
    "coverage",
    ".vscode",
    "vendor",
    ".svn",
    ".git",
    ".hg",
    "*.sassc",
    "CVS",
    ".sass-cache",
    ".idea",
    ".stencil"
  ],
  "font_face": "Source code pro",
  "font_options": "no_liga",
  "font_size": 10,
  "ignored_packages": [
    "Autocomplete Javascript with Method Signature",
    "DocBlockr",
    "Theme - Soda",
    "Vintage"
  ],
  "line_padding_bottom": 0,
  "material_theme_accent_blue": true,
  "material_theme_accent_scrollbars": true,
  "material_theme_accent_titlebar": false,
  "material_theme_appbar_charcoal": true,
  "material_theme_arrow_folders": true,
  "material_theme_compact_panel": true,
  "material_theme_compact_sidebar": true,
  "material_theme_contrast_mode": true,
  "material_theme_small_statusbar": true,
  "material_theme_small_tab": true,
  "material_theme_tabs_autowidth": true,
  "material_theme_titlebar": true,
  "predawn_findreplace_small": false,
  "predawn_quick_panel_small": false,
  "predawn_tabs_active_underline": false,
  "predawn_tabs_small": true,
  "tab_size": 2,
  "theme": "Spacegray Eighties.sublime-theme",
  "translate_tabs_to_spaces": true,
  "typescript_plugin_tsc_version": "2.5.3",
  "use_tab_stops": true,
  "word_wrap": "true"
}
