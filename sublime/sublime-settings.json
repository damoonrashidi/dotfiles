{
  "always_show_minimap_viewport": true,
  "auto_complete": true,
  "bold_folder_labels": true,
  "caret_extra_bottom": 0,
  "caret_extra_top": 0,
  "caret_extra_width": 8,
  "caret_style": "phase",
  "color_scheme": "Packages/User/freespee.tmTheme",
  "dart_sdk_path": "/usr/local/opt/dart/libexec",
  "detect_indentation": true,
  "draw_indent_guides": false,
  "draw_white_space": false,
  "enable_tab_scrolling": false,
  "file_exclude_patterns": [
    "*.pyc",
    "*.pyo",
    "*.exe",
    "*.dll",
    "*.obj",
    "*.o",
    "*.a",
    "*.lib",
    "*.so",
    "*.dylib",
    "*.ncb",
    "*.sdf",
    "*.suo",
    "*.pdb",
    "*.idb",
    ".DS_Store",
    "*.class",
    "*.psd",
    "*.db",
    "*.sassc",
    "*.scssc",
    "*.map",
    "*.po",
    "*.mo",
    "yarn.lock",
    "package.lock"
  ],
  "folder_exclude_patterns": [
    ".fusebox",
    "node_modules",
    "build",
    "output",
    "typings",
    "dist",
    "coverage",
    ".vscode",
    "vendor",
    ".svn",
    ".git",
    ".hg",
    "*.sassc",
    "CVS",
    ".sass-cache",
    ".idea",
    ".stencil",
    ".cache",
    "cypress",
    "documentation"
  ],
  "font_face": "Inconsolata",
  "font_options": "no_liga",
  "font_size": 13,
  "ignored_packages": [
    "DocBlockr",
    "Go To Definition",
    "Theme - Soda",
    "Vintage"
  ],
  "line_padding_bottom": 0,
  "search_case_sensitivity": false,
  "tab_size": 2,
  "theme": "ayu-dark.sublime-theme",
  "translate_tabs_to_spaces": true,
  "typescript_plugin_tsc_version": "3.3.3",
  "enable_inline_error_tips": true,
  "use_tab_stops": true,
  "word_wrap": true
}
