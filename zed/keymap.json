[
  {
    "bindings": {
      "cmd-r": "outline::Toggle"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-shift-d": "editor::DuplicateLineDown",
      "alt-ctrl-up": "editor::GoToPrevHunk",
      "alt-ctrl-down": "editor::GoToHunk",
      "f11": "editor::FindAllReferences",
      "cmd-[": "pane::GoBack",
      "cmd-]": "pane::GoForward",
      "alt-up": "editor::SelectLargerSyntaxNode",
      "alt-down": "editor::SelectSmallerSyntaxNode",
      "cmd-ctrl-down": "editor::MoveLineDown",
      "cmd-ctrl-up": "editor::MoveLineUp",
      "cmd-shift-g": "editor::SelectAllMatches"
    }
  },
  {
    "context": "ProjectPanel",
    "bindings": {
      "cmd-.": "project_panel::NewFile",
      "cmd-,": "project_panel::NewDirectory",
      "f2": "project_panel::Rename",
      "cmd-shift-c": "project_panel::CopyPath",
      "enter": "project_panel::Open"
    }
  }
]
