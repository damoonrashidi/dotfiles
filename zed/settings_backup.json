{
  "show_inline_completions": true,
  "features": {
    "inline_completion_provider": "zed"
  },
  "vim_mode": false,
  "language_models": {
    "openai": {
      "version": "1"
    }
  },
  "theme": {
    "mode": "system",
    "dark": "Rosé Pine Moon",
    "light": "Rosé Pine Moon"
  },
  "indent_guides": {
    "enabled": false
  },
  "when_closing_with_no_tabs": "keep_window_open",
  "task": {
    "show_status_indicator": false
  },
  "collaboration_panel": {
    "dock": "right"
  },
  "preview_tabs": {
    "enabled": false
  },
  "outline_panel": {
    "button": false,
    "dock": "right"
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false
  },
  "gutter": {
    "line_numbers": false,
    "code_actions": false
  },
  "ui_font_size": 11,
  "buffer_font_size": 11,
  "buffer_line_height": {
    "custom": 2.5
  },
  "buffer_font_family": "Monaspace Krypton",
  "buffer_font_features": {
    "calt": true,
    "liga": true
  },
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "enabled": true,
    "enable_experimental_live_diffs": true,
    "dock": "right",
    "version": "2"
  },
  "inlay_hints": {
    "enabled": false
  },
  "project_panel": {
    "dock": "left",
    "auto_fold_dirs": false,
    "indent_guides": {
      "show": "never"
    },
    "indent_size": 15,
    "button": false
  },
  "format_on_save": "on",
  "formatter": {
    "language_server": {
      "name": "prettier"
    }
  },
  "journal": {
    "path": "~",
    "hour_format": "hour24"
  },
  "terminal": {
    "copy_on_select": true,
    "dock": "right",
    "default_width": 200,
    "line_height": {
      "custom": 1.0
    }
  },
  "chat_panel": {
    "button": "when_in_call"
  },
  "notification_panel": {
    "button": false
  },
  "languages": {
    "GraphQL": {
      "formatter": "prettier"
    },
    "JSON": {
      "formatter": "prettier"
    },
    "Markdown": {
      "formatter": "prettier"
    },
    "Svelte": {
      "formatter": "prettier",
      "code_actions_on_format": {
        "source.fixAll": true,
        "source.fixAll.eslint": true
      }
    },
    "TypeScript": {
      "formatter": "prettier",
      "code_actions_on_format": {
        "source.fixAll": true,
        "source.fixAll.eslint": true
      },
      "format_on_save": {
        "external": {
          "command": "prettier",
          "arguments": [
            "--stdin-filepath",
            "{buffer_path}"
          ]
        }
      }
    },
    "TSX": {
      "code_actions_on_format": {
        "source.fixAll": true,
        "source.fixAll.eslint": true
      },
      "format_on_save": {
        "external": {
          "command": "prettier",
          "arguments": [
            "--stdin-filepath",
            "{buffer_path}"
          ]
        }
      }
    }
  },
  "file_scan_inclusions": [
    ".env"
  ],
  "code_actions_on_format": {
    "source.fixAll.eslint": true,
    "source.organizeImports.eslint": true
  },
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "checkOnSave": {
          "command": "clippy"
        }
      }
    },
    "biome": {
      "settings": {
        "enabled": false,
        "require_config_file": true
      }
    },
    "eslint": {
      "enabled": true,
      "autoFixOnSave": true,
      "autoFixOnFormat": true,
      "autoFixOnFormatDelay": 200,
      "initialization_options": {
        "checkOnSave": {
          "command": "eslint"
        }
      },
      "probe": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "html",
        "svelte"
      ],
      "validate": [
        "javascript",
        "typescript",
        "javascriptreact",
        "svelte"
      ]
    }
  }
}
